{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/index.html",
          "/*.css",
          "/*.js",
          "/favicon.ico"
        ],
        "urls": [
          "/assets/css/**",
          "/assets/js/**"
        ]
      }
    },
    {
      "name": "images",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/img/**"
        ]
      }
    }
  ]
}
