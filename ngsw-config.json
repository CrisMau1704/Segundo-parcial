{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**/*.svg",
          "/assets/**/*.jpg",
          "/assets/**/*.jpeg",
          "/assets/**/*.png",
          "/assets/**/*.gif",
          "/assets/**/*.webp",
          "/assets/**/*.woff",
          "/assets/**/*.woff2",
          "/assets/**/*.ttf",
          "/assets/**/*.otf"
        ]
      }
    }
  ]
}
